<script src="http://code.jquery.com/jquery-1.11.3.min.js"></script>
<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.21/css/jquery.dataTables.css">

<script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.21/js/jquery.dataTables.js"></script>

@{
    ViewBag.Title = "WayBills";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    #WayBillsGrid td:nth-child(1) {
        display: none;
    }

    #WayBillsGrid {
        z-index: 6;
    }
</style>
<br />
<br />
<br />
<br />
<br />

<div>
    <h2 style="text-align:center">WayBills</h2>
    <br />
    <br />
    <br />
    <br />
    <br />
    <table id="WayBillsGrid" class="table table-striped table-bordered" style="width:100%;z-index:6">
        <thead>
            <tr>
                <th style="display:none">Id</th>
                <th>Weight</th>
                <th>No of Parcels</th>
                <th>Vehicle Id</th>

            </tr>
        </thead>

    </table>
    <br />
    <div class="row" style="margin-left:15px;display:flex; justify-content:flex-end; width:100%; padding:0; float:right;z-index:10">

        <div class="col-md-1">
            <button class="btn btn-success btn-block" onclick="SaveWayBillData()">Save</button>
        </div>

        <br />
        <br />
        <br />
        <br />
        <br />

    </div>
</div>

@section scripts{
<script src="~/Scripts/jquery.min.js"></script>
<script type="text/javascript" src="~/Scripts/jquery.dataTables.min.js"></script>
<script src="~/Scripts/Vehicle.js"></script>
<script src="~/Scripts/bootoast.js"></script>
}
